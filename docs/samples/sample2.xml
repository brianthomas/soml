      xsi:schemaLocation="http://www.data-model.net/SemanticObject SOML_55.xsd"
      URI="urn:test:SemanticObject1"
>

  <!-- in the relationship1 with an object -->
  <so:relationship URI="urn:test:rel1">
      <so:semanticObject URI="urn:test:SemanticObject2"/>
