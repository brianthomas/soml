<?xml version="1.0" standalone="no" ?>
<!-- cross ref'ing example between 2 SO's -->
<so:semanticObject 
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
      xmlns:so="http://www.data-model.net/SemanticObject" 
      xmlns:a="http://test.org/onto/Test1" 
      xmlns:b="http://test.org/onto/Test2" 
      xsi:schemaLocation="http://www.data-model.net/SemanticObject SOML_56.xsd"
      URI="urn:test:SemanticObject1"
>

  <!-- in the property1 with an obj2 -->
  <a:prop12>
      <so:semanticObject soId="obj2" URI="urn:test:SemanticObject2">
     	   <!-- obj2 has property to object 3 -->
		  <b:prop23>
		      <so:semanticObjectRef soIdRef="obj3"/>
		  </b:prop23>
      </so:semanticObject>
  </a:prop12>
  
  <!-- in property2 with obj3 -->
  <a:prop13>
      <so:semanticObject soId="obj3" URI="urn:test:SemanticObject2">
     	   <!-- obj3 has property to object 2 -->
		  <b:prop32>
		      <so:semanticObjectRef soIdRef="obj2"/>
		  </b:prop32>
      </so:semanticObject>
  </a:prop13>
  
</so:semanticObject>